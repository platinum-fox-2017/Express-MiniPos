<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>SEARCH</title>
  </head>
  <%include ./partials/header%>
  <body>
    <h4>ITEM SEARCH</h4>
    <br>
    <form class="" action="" method="post">
      <input type="test" name="itemName" placeholder="itemName">
      <input type="text" name="min_value" placeholder="min">
      <input type="text" name="max_value" placeholder="max">
      <input type="submit" name="" value="SEARCH">
    </form>
    <br>
    <h4>HASIL SEARCH</h4>
    <br>
    <table border="1">
      <thead>
        <td>No.</td>
        <td>ITEM NAME</td>
        <td>SUPPLIER</td>
        <td>PRICE</td>
      </thead>
      <%dataItems.forEach((data,index)=>{%>
        <tr>
          <td><%=index+1%></td>
          <td><%=data.Item.name%></td>
          <td><%=data.Supplier.name%></td>
          <td><%=helper(data.price)%></td>
        </tr>
      <%})%>
    </table>
  </body>
</html>
