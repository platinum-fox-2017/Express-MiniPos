<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>SUPPLIERS</title>
  </head>
  <h2>SUPPLIERS</h2>
  <body>
    <form action="/suppliers/add" method="post">
      <input type="text" name="name" value="" placeholder="your name here">
      <input type="text" name="kota" value="" placeholder="your city here">
      <input type="submit" name="" value="ADD NEW SUPPLIER">
    </form>
    <table border = '1'>
      <thead>
        <td>ID</td>
        <td>NAME</td>
        <td>KOTA</td>
      </thead>
      <%dataSuppliers.forEach(data=>{%>
        <tr>
          <td><%=data.id%></td>
          <td><%=data.name%></td>
          <td><%=data.kota%></td>
          <td> <a href="/suppliers/update/<%=data.id%>"><button type="button" name="button"> UPDATE </button></a></td>
          <td>
            <a href="/suppliers/delete/<%=data.id%>">DELETE</a>
          </td>
        </tr>
      <%})%>
    </table>
  </body>
</html>
