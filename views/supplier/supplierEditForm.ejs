<html>
<head>
  <meta charset="utf-8">
  <link rel="stylesheet" type="text/css" href="/styles/css.css">
  <title><%= title %></title>
</head>

<body>
  <%- include('../partial/navigation.ejs') %>
  <div class="main">
    <div class="content">
      <h1><%= header %></h1>
      <% if (err) { %>
        <h4><%= err.message %><h4>
      <% } %>
      <form action="/suppliers/edit/<%= supplierId %>" method="post">
        Name:<br>
        <input type="text" name="name" value="<%= foundSupplier.name %>"><br>
        <br>
        kota:<br>
        <input type="text" name="kota" value="<%= foundSupplier.kota %>"><br>
        <br>
        <input type="submit" value="Submit">
      </form>
    </div>
  </div>

</body>

</html>
